		"passport-jwt": "^4.0.0",
		"reflect-metadata": "^0.1.13",
		"rimraf": "^3.0.2",
		"rxjs": "^7.5.2",
		"sharp": "^0.31.2",
		"telegraf": "^4.10.0",
		"xml2js": "^0.4.23"
	},
	"devDependencies": {
		"@nestjs/cli": "^9.1.5",
		"@nestjs/schematics": "^9.0.3",
		"@nestjs/testing": "^9.2.0",
		"@types/app-root-path": "^1.2.4",
		"@types/bcryptjs": "^2.4.2",
		"@types/cron": "^2.0.0",
		"@types/express": "^4.17.14",
		"@types/fs-extra": "^9.0.13",
		"@types/jest": "^29.2.2",
		"@types/mongoose": "^5.11.97",
		"@types/multer": "^1.4.7",
		"@types/node": "^18.11.9",
		"@types/passport-jwt": "^3.0.7",
		"@types/sharp": "^0.31.0",
		"@types/supertest": "^2.0.12",
		"@types/xml2js": "^0.4.11",
		"jest": "^29.3.0",
		"prettier": "^2.7.1",
		"supertest": "^6.3.1",
		"ts-jest": "^29.0.3",
		"ts-loader": "^9.4.1",
		"ts-node": "^10.9.1",
		"tsconfig-paths": "^4.1.0",
		"tslint": "^6.1.3",
		"typescript": "^4.8.4"
	},
	"jest": {
		"moduleFileExtensions": [
			"js",
			"json",
			"ts"
		],
		"rootDir": "src",
		"testRegex": ".*\\.spec\\.ts$",
		"transform": {
			"^.+\\.(t|j)s$": "ts-jest"
		},
		"collectCoverageFrom": [
			"**/*.(t|j)s"
		],
		"coverageDirectory": "../coverage",
		"testEnvironment": "node"
